<!--poor solution to get out of page-->
<a href="/" id="home-link">Back to Dash</a>
<div class="full-app">

	{{#if isStar}}
		<button class="unstar-custom-btn btn btn-primary btn-lg" data-custom-id="{{customId}}" data-listing-id="{{listing._id}}" data-user-id="{{owner._id}}">
			<span class='glyphicon glyphicon-star center' style="color:yellow"></span>
		</button>
	{{else}}
		<button class="star-custom-btn btn btn-primary btn-lg" data-custom-id="{{customId}}" data-listing-id="{{listing._id}}" data-user-id="{{owner._id}}">
			<span class='glyphicon glyphicon-star center'></span>
		</button>
	{{/if}}

	<button id="reject-custom-btn" class="btn btn-primary" data-custom-id="{{customId}}" data-listing-id="{{listing._id}}" data-user-id="{{owner._id}}">
		Reject
	</button>

	<h3>Common Application</h3>
	{{> application common isSubmitted=isSubmitted isCommon=true}}
	<h3>Custom Application</h3>
	{{> application custom isSubmitted=isSubmitted isCommon=false}}
</div>