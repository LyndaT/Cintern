<div class="s_listings">
  <a href="/students">Back to Dash</a>

  <h1>Current available listings</h1>
  <p>Click on a listing for more detailed information.</p>
  <br>

  <table class="table table-hover table-condensed row-clickable">

    <thead>
      <th>Company</th>
      <th>Position</th>
      <th>Description</th>
    </thead>

    <tbody>
      {{#each listings}}
        {{> listing}}
      {{else}}
        <tr>
          <td>No listings available</td>
        </tr>
      {{/each}}
    </tbody>

  </table>
</div>
