<div class="s_listings">

  <a href="/students">Back to Dash</a>

  <h1>Current available listings</h1>
  <p>Click on a listing for more detailed information.</p>
  <br>

  <table class="table table-hover table-condensed">

    <tr>
      <th>Company</th>
      <th>Position</th>
      <th>Description</th>
    </tr>

    {{#each listings}}
      {{> listing}}
    {{else}}
      <p><em>No listings available</em></p>
    {{/each}}

  </table>
</div>
